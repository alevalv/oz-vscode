{
    "name": "oz-vscode",
    "displayName": "MOzArt for VSCode",
    "description": "MOzArt Language Support for Visual Studio Code",
    "version": "0.0.5",
    "publisher": "alevalv",
    "license": "GNU GPLv3",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "homepage": "https://github.com/alevalv/oz-vscode",
    "repository": {
        "type": "git",
        "url": "https://github.com/alevalv/oz-vscode.git"
    },
    "icon": "images/icon.png",
    "bugs": "https://github.com/alevalv/oz-vscode/issues",
    "contributes": {
        "languages": [{
            "id": "oz",
            "aliases": ["Oz", "MOzArt"],
            "extensions": [".oz", ".mozart"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [
            {
                "language": "oz",
                "scopeName": "source.oz",
                "path": "./syntaxes/oz.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "oz",
                "path": "./snippets/oz.json"
            }
        ]
    }
}
